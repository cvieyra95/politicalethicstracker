{
    "name": "PoliWatch",
    "description": "Political ethics tracker",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html",
      "default_icon": "Logo.png"
    },
    "content_scripts": [
      {
        "matches":["<all_urls>"],
        "js": ["scan.js"],
        "css": ["scan.css"]
      }
    ]
  }